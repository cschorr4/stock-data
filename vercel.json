{
    "version": 2,
    "buildCommand": "next build",
    "framework": "nextjs",
    "installCommand": "npm install",
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      },
      {
        "src": "requirements.txt",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }